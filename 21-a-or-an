public String aOrAn(String word) {
    //code here
    String letra= String.valueOf(word);
    If(letra.startsWith('a')||
      letra.startsWith('e')||
      letra.startsWith('i')||
      letra.startsWith('o')||
      letra.startsWith('u')){
        String frase= 'an '+ word;
        return frase;
    }else{
        String frase= 'a '+ word;
        return frase;
    }
}